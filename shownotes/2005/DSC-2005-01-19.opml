    <outline text="Sound Seeing">
      <outline text="Mobcasting">
        <outline text="From:    Andy Carvin &lt;andycarvin@yahoo.com&gt;"/>
        <outline text="Subject:   An experiment in &quot;mobcasting&quot;"/>
        <outline text="Date:  January 18, 2005 5:07:41 PM GMT+00:00"/>
        <outline text="To:    Adam Curry &lt;adam@curry.com&gt;"/>
        <outline text=""/>
        <outline text="I've just created a new site to be used for"/>
        <outline text="experiments in mobcasting: giving smart mobs access to"/>
        <outline text="a community podcasting tool accessible through their"/>
        <outline text="mobile phones. "/>
        <outline text=""/>
        <outline text="http://mobcasting.blogspot.com"/>
        <outline text=""/>
        <outline text="The first experiment is a mobcasting tool for"/>
        <outline text="protestors at this week's presidential inauguration:"/>
        <outline text=""/>
        <outline text="http://bushprotest.blogspot.com/"/>
        <outline text=""/>
        <outline text="Protestors (and anyone else) attending the"/>
        <outline text="inauguration will be able to call a telephone number"/>
        <outline text="and post podcasts on-the-fly to the same website... -andy"/>
        <outline text=""/>
        <outline text="__________________________________________________"/>
        <outline text="Do You Yahoo!?"/>
        <outline text="Tired of spam?  Yahoo! Mail has the best spam protection around "/>
        <outline text="http://mail.yahoo.com "/>
      </outline>
      <outline artist="Alan Joyce" text="Alan Joyce" type="artist">
